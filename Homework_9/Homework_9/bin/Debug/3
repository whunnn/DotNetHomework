$ (document).ready(function(){
	
//Script for OpenEnd box alignment in Radio	

if ( $ (".question").length == 1 && $ (".question.radio").length == 1 && $ (".question.radio").hasClass("noRows") && $ (".question.radio").hasClass("noCols") && $ (".question").find(".sq-atm1d-block").length > 0) {
	
